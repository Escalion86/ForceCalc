{
  "expo": {
    "name": "ForceCalc",
    "slug": "ForceCalc",
    "version": "1.4.2",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "forcecalc.escalion.ru"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "32f76ceb-abca-49d1-b334-c86dee3bdcbd"
      }
    },
    "plugins": ["expo-font"]
  }
}
